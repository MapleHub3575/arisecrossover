local DungeontTab = Window:CreateTab("Dungeont", 4483362458)
local DungeontSection = DungeontTab:CreateSection("Dungeont Settings")

------------------------------------------------------
-- üå∏ Auto Dungeont (Teleport when Dungeon appears)
------------------------------------------------------

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function startAutoDungeon()
	task.spawn(function()
		local dungeonFolder = workspace:WaitForChild("__Main"):WaitForChild("__Dungeon")

		while AutoDungeontToggle.CurrentValue do
			local character = player.Character or player.CharacterAdded:Wait()
			local hrp = character:WaitForChild("HumanoidRootPart")

			-- ‡∏ï‡∏£‡∏ß‡∏à‡∏ß‡πà‡∏≤‡∏°‡∏µ Dungeon ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß‡πÑ‡∏´‡∏°
			local dungeonPart = dungeonFolder:FindFirstChild("Dungeon")
			if dungeonPart then
				-- ‡∏ß‡∏≤‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡πÄ‡∏•‡∏¢
				hrp.CFrame = dungeonPart.CFrame * CFrame.new(0, 5, 0)
				return
			end

			-- ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ ‡πÉ‡∏´‡πâ‡∏£‡∏≠‡∏à‡∏ô‡∏ñ‡∏π‡∏Å‡∏™‡∏£‡πâ‡∏≤‡∏á
			local newPart = dungeonFolder.ChildAdded:Wait()
			if newPart.Name == "Dungeon" and AutoDungeontToggle.CurrentValue then
				local character = player.Character or player.CharacterAdded:Wait()
				local hrp = character:WaitForChild("HumanoidRootPart")
				hrp.CFrame = newPart.CFrame * CFrame.new(0, 5, 0)
				return
			end
		end
	end)
end

------------------------------------------------------
-- üå∏ Toggle UI
------------------------------------------------------
AutoDungeontToggle = DungeontTab:CreateToggle({
	Name = "Auto Dungeont",
	CurrentValue = false,
	Flag = "AutoDungeont",
	Callback = function(Value)
		if Value then
			startAutoDungeon()
		end
	end,
})

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local enemiesFolder = workspace.__Main.__Enemies.Server
local AutoFarmEnabled = false
local farmSpeed = 3

-- üåÄ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤ Part ‡∏®‡∏±‡∏ï‡∏£‡∏π
local function teleportToEnemyPart(enemyPart, offsetDistance)
	if not (enemyPart and enemyPart.Parent and enemyPart:IsDescendantOf(workspace)) then return end
	if not (humanoidRootPart and humanoidRootPart.Parent) then return end

	local direction = (enemyPart.Position - humanoidRootPart.Position).Unit
	local distance = (enemyPart.Position - humanoidRootPart.Position).Magnitude
	local targetPos = enemyPart.Position - direction * offsetDistance

	-- üí° ‡∏ñ‡πâ‡∏≤‡πÑ‡∏Å‡∏•‡πÄ‡∏Å‡∏¥‡∏ô 200 stud ‡πÉ‡∏´‡πâ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡πÅ‡∏ó‡∏ô Tween
	if distance > 300 then
		humanoidRootPart.CFrame = CFrame.new(targetPos)
	else
		local tweenTime = math.clamp(1 / farmSpeed, 0.1, 1)
		local tweenInfo = TweenInfo.new(tweenTime, Enum.EasingStyle.Linear)
		local tween = TweenService:Create(humanoidRootPart, tweenInfo, {CFrame = CFrame.new(targetPos)})
		tween:Play()
		task.wait(tweenTime)
	end
end

-- üîé ‡∏´‡∏≤ Part ‡∏®‡∏±‡∏ï‡∏£‡∏π‡∏ó‡∏µ‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î (‡∏Å‡∏£‡∏≠‡∏á HP<=0)
local function getClosestEnemyPart()
	local closestPart = nil
	local shortestDistance = math.huge

	for _, part in ipairs(enemiesFolder:GetChildren()) do
		if part:IsA("Part") and part:IsDescendantOf(workspace) then
			local hp = part:GetAttribute("HP")
			if hp and hp > 0 then -- ‚úÖ ‡∏≠‡πà‡∏≤‡∏ô‡∏à‡∏≤‡∏Å Attribute
				local distance = (part.Position - humanoidRootPart.Position).Magnitude
				if distance < shortestDistance then
					shortestDistance = distance
					closestPart = part
				end
			end
		end
	end

	return closestPart
end

-- ‚öîÔ∏è Auto Farm ‡∏´‡∏•‡∏±‡∏Å
local function autoFarm()
	while AutoFarmEnabled do
		local enemyPart = getClosestEnemyPart()
		if enemyPart then
			teleportToEnemyPart(enemyPart, 5)

			-- ‚úÖ ‡πÉ‡∏ä‡πâ Attribute ‡πÅ‡∏ó‡∏ô FindFirstChild
			while AutoFarmEnabled do
				local hp = enemyPart:GetAttribute("HP")
				if not hp or hp <= 0 then
					break -- ‡∏ï‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß ‡∏´‡∏¢‡∏∏‡∏î‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ï‡∏±‡∏ß‡∏ô‡∏µ‡πâ
				end
				task.wait(0.05)
			end
		else
			task.wait(0.5) -- ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏®‡∏±‡∏ï‡∏£‡∏π
		end
		task.wait(0.05)
	end
end

-- üîÅ ‡∏£‡∏µ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡∏´‡∏•‡∏±‡∏á‡∏£‡∏µ‡πÄ‡∏Å‡∏¥‡∏î
player.CharacterAdded:Connect(function(newChar)
	task.wait(1)
	character = newChar
	humanoidRootPart = newChar:WaitForChild("HumanoidRootPart")
	if AutoFarmEnabled then
		task.spawn(autoFarm)
	end
end)

-- üå∏ ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î‡∏õ‡∏¥‡∏î Auto Farm
DungeontTab:CreateToggle({
	Name = "Auto Farm",
	CurrentValue = false,
	Flag = "AutoFarm",
	Callback = function(Value)
		AutoFarmEnabled = Value
		if Value then
			task.spawn(autoFarm)
		end
	end,
})

-- ‚ö° ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏ü‡∏≤‡∏£‡πå‡∏°
DungeontTab:CreateSlider({
	Name = "Farm Speed",
	Range = {1, 10},
	Increment = 1,
	CurrentValue = farmSpeed,
	Flag = "FarmSpeed",
	Callback = function(Value)
		farmSpeed = Value
	end,
})


local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local Remote = ReplicatedStorage:WaitForChild("BridgeNet2"):WaitForChild("dataRemoteEvent")

-- ‚úÖ ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏´‡∏•‡∏±‡∏Å
local AutoJoinEnabled = false
local AutoReplayEnabled = false
local joinRange = 25 -- ‡∏£‡∏∞‡∏¢‡∏∞‡πÉ‡∏Å‡∏•‡πâ‡∏õ‡∏£‡∏∞‡∏ï‡∏π‡∏î‡∏±‡∏ô
local enemiesFolder = Workspace:WaitForChild("__Main"):WaitForChild("__Enemies"):WaitForChild("Server")

-- üå∏ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏±‡πà‡∏ß‡πÑ‡∏õ
local function isInDungeon()
	for _, obj in ipairs(Workspace:GetDescendants()) do
		if obj:IsA("Part") and obj:FindFirstChild("Type") and obj.Type.Value == "Dungeon" then
			return true
		end
	end
	return false
end

local function isNearDungeonDoor()
	local door = Workspace:FindFirstChild("__Main")
	if door and door:FindFirstChild("__Dungeon") then
		local dungeon = door.__Dungeon:FindFirstChild("Dungeon")
		if dungeon and humanoidRootPart then
			local distance = (humanoidRootPart.Position - dungeon.Position).Magnitude
			return distance <= joinRange
		end
	end
	return false
end

-- üå∏ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏Å‡∏µ‡πà‡∏¢‡∏ß‡∏Å‡∏±‡∏ö‡∏î‡∏±‡∏ô‡πÄ‡∏à‡∏µ‡πâ‡∏¢‡∏ô
local function buyTicket()
	local args = {
		[1] = {
			[1] = {
				["Type"] = "Gems",
				["Event"] = "DungeonAction",
				["Action"] = "BuyTicket"
			},
			[2] = "\3"
		}
	}
	Remote:FireServer(unpack(args))
end

local function createDungeon()
	local args = {
		[1] = {
			[1] = {
				["Event"] = "DungeonAction",
				["Action"] = "Create"
			},
			[2] = "\3"
		}
	}
	Remote:FireServer(unpack(args))
end

local function startDungeon()
	local args = {
		[1] = {
			[1] = {
				["Event"] = "DungeonAction",
				["Action"] = "Start"
			},
			[2] = "\3"
		}
	}
	Remote:FireServer(unpack(args))
end

-- üå∏ ‡∏£‡∏∞‡∏ö‡∏ö Auto Join Dungeon
DungeontTab:CreateToggle({
	Name = "Auto Join Dungeon",
	CurrentValue = false,
	Flag = "AutoJoinDungeon",
	Callback = function(Value)
		AutoJoinEnabled = Value
		if Value then
			task.spawn(function()
				while AutoJoinEnabled do
					if not isInDungeon() then
						if isNearDungeonDoor() then
							buyTicket()
							task.wait(1)
							createDungeon()
							task.wait(1)
							startDungeon()
							task.wait(5)
						end
					end
					task.wait(3)
				end
			end)
		end
	end,
})

-- üå∏ ‡∏£‡∏∞‡∏ö‡∏ö Auto Replay (‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏°‡∏≠‡∏ô‡πÑ‡∏°‡πà‡πÄ‡∏´‡∏•‡∏∑‡∏≠)
local function autoReplay()
	while AutoReplayEnabled do
		-- ‡∏ï‡∏£‡∏ß‡∏à‡∏ß‡πà‡∏≤‡∏°‡∏µ‡∏°‡∏≠‡∏ô‡πÄ‡∏´‡∏•‡∏∑‡∏≠‡πÑ‡∏´‡∏°
		local enemiesAlive = false
		for _, enemy in ipairs(enemiesFolder:GetChildren()) do
			if enemy:IsA("Model") or enemy:IsA("Part") then
				enemiesAlive = true
				break
			end
		end

		-- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏°‡∏≠‡∏ô‡πÄ‡∏´‡∏•‡∏∑‡∏≠ ‚Üí ‡∏£‡∏≠‡πÄ‡∏ä‡πá‡∏Å‡∏ã‡πâ‡∏≥‡∏≠‡∏µ‡∏Å‡∏£‡∏≠‡∏ö‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÉ‡∏´‡∏°‡πà
		if not enemiesAlive then
			task.wait(3) -- ‚úÖ ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏ä‡πá‡∏Å‡∏ã‡πâ‡∏≥‡∏Å‡∏±‡∏ô‡∏ö‡∏±‡πä‡∏Å
			local stillNoEnemies = true
			for _, enemy in ipairs(enemiesFolder:GetChildren()) do
				if enemy:IsA("Model") or enemy:IsA("Part") then
					stillNoEnemies = false
					break
				end
			end

			if stillNoEnemies then
				buyTicket()
				task.wait(1)
				createDungeon()
				task.wait(2)
				startDungeon()
				task.wait(5)
			end
		end

		task.wait(3)
	end
end

DungeontTab:CreateToggle({
	Name = "Auto Replay",
	CurrentValue = false,
	Flag = "AutoReplay",
	Callback = function(Value)
		AutoReplayEnabled = Value
		if Value then
			task.spawn(autoReplay)
		end
	end,
})
