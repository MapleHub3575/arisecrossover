local SellpetSection =3D INFOSTab:CreateSection("Auto Sell Settings")

local AutoSellEnabled =3D false
local connection -- =E0=B9=84=E0=B8=A7=E0=B9=89=E0=B9=80=E0=B8=81=E0=B9=87=
=E0=B8=9A event =E0=B9=80=E0=B8=9E=E0=B8=B7=E0=B9=88=E0=B8=AD=E0=B8=AB=E0=
