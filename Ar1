INFOSTab:CreateToggle({
    Name =3D "Auto Sell (Rank E-UGM+)",
    CurrentValue =3D false,
    Flag =3D "AutoSell",
    Callback =3D function(Value)
        AutoSellEnabled =3D Value

        if AutoSellEnabled then
            -- =E0=B8=AB=E0=B8=B2=E0=B9=82=E0=B8=9F=E0=B8=A5=E0=B9=80=E0=
=B8=94=E0=B8=AD=E0=B8=A3=E0=B9=8C Pets
            local petsFolder =3D player:FindFirstChild("leaderstats")
            if petsFolder then
                petsFolder =3D petsFolder:FindFirstChild("Inventory")
                if petsFolder then
                    petsFolder =3D petsFolder:FindFirstChild("Pets")
                end
            end

            if not petsFolder then
                return
            end

            -- =E0=B8=9F=E0=B8=B1=E0=B8=87=E0=B8=81=E0=B9=8C=E0=B8=8A=E0=
=B8=B1=E0=B8=99=E0=B8=82=E0=B8=B2=E0=B8=A2 pet =E0=B8=97=E0=B8=B5=E0=B9=88=
 rank 1=E2=80=9310
            local function sellLowRankPets(pet)
                if not AutoSellEnabled then return end
                if pet and pet:IsA("Folder") then
                    local rankValue =3D pet:GetAttribute("Rank")
                    if typeof(rankValue) =3D=3D "number" and rankValue >=
=3D 1 and rankValue <=3D 16 then
                        local args =3D {
                            [1] =3D {
                                [1] =3D {
                                    ["Event"] =3D "SellPet",
                                    ["Pets"] =3D { pet.Name }
                                },
                                [2] =3D "\7"
                            }
                        }
                        BridgeEvent:FireServer(unpack(args))
                    end
                end
            end

            -- =E0=B8=82=E0=B8=B2=E0=B8=A2 pet =E0=B8=97=E0=B8=B5=E0=B9=88=
=E0=B8=A1=E0=B8=B5=E0=B8=AD=E0=B8=A2=E0=B8=B9=E0=B9=88=E0=B9=81=E0=B8=A5=
=E0=B9=89=E0=B8=A7 (=E0=B8=96=E0=B9=89=E0=B8=B2=E0=B8=A1=E0=B8=B5)
            for =5F, pet in ipairs(petsFolder:GetChildren()) do
                sellLowRankPets(pet)
            end

            -- =E0=B8=95=E0=B8=A3=E0=B8=A7=E0=B8=88=E0=B8=97=E0=B8=B8=E0=
=B8=81=E0=B8=84=E0=B8=A3=E0=B8=B1=E0=B9=89=E0=B8=87=E0=B8=97=E0=B8=B5=E0=
=B9=88=E0=B8=A1=E0=B8=B5 pet =E0=B9=83=E0=B8=AB=E0=B8=A1=E0=B9=88=E0=B9=80=
=E0=B8=9E=E0=B8=B4=E0=B9=88=E0=B8=A1=E0=B9=80=E0=B8=82=E0=B9=89=E0=B8=B2=
=E0=B8=A1=E0=B8=B2
            connection =3D petsFolder.ChildAdded:Connect(function(pet)
                sellLowRankPets(pet)
            end)

        else
            -- =E0=B8=9B=E0=B8=B4=E0=B8=94=E0=B8=A3=E0=B8=B0=E0=B8=9A=E0=
=B8=9A auto sell
            if connection then
                connection:Disconnect()
                connection =3D nil
            end
        end
    end,
})

