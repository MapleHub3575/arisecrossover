        local player =3D game.Players.LocalPlayer
      local Players =3D game:GetService("Players")
local TweenService =3D game:GetService("TweenService")
local ReplicatedStorage =3D game:GetService("ReplicatedStorage")
local Workspace =3D game:GetService("Workspace")

local player =3D Players.LocalPlayer
local character =3D player.Character or player.CharacterAdded:Wait()
local humanoidRootPart =3D character:WaitForChild("HumanoidRootPart")

-- =F0=9F=94=81 =E0=B9=82=E0=B8=9F=E0=B8=A5=E0=B9=80=E0=B8=94=E0=B8=AD=E0=
=B8=A3=E0=B9=8C=E0=B8=A8=E0=B8=B1=E0=B8=95=E0=B8=A3=E0=B8=B9
local enemiesFolder =3D workspace:WaitForChild("=5F=5FMain"):WaitForChild(=
"=5F=5FEnemies"):WaitForChild("Server")

-- =E2=9A=99=EF=B8=8F =E0=B8=95=E0=B8=B1=E0=B8=A7=E0=B9=81=E0=B8=9B=E0=B8=
=A3=E0=B8=AB=E0=B8=A5=E0=B8=B1=E0=B8=81
local AutoFarmEnabled =3D false
local farmSpeed =3D 5
local currentTween

--------------------------------------------------------
-- =F0=9F=A7=AD =E0=B8=9F=E0=B8=B1=E0=B8=87=E0=B8=81=E0=B9=8C=E0=B8=8A=E0=
=B8=B1=E0=B8=99=E0=B8=8A=E0=B9=88=E0=B8=A7=E0=B8=A2=E0=B8=9E=E0=B8=B7=E0=
=B9=89=E0=B8=99=E0=B8=90=E0=B8=B2=E0=B8=99
--------------------------------------------------------

-- =F0=9F=94=B9 =E0=B8=94=E0=B8=B6=E0=B8=87 HumanoidRootPart =E0=B8=9B=E0=
=B8=A5=E0=B8=AD=E0=B8=94=E0=B8=A0=E0=B8=B1=E0=B8=A2 (=E0=B8=81=E0=B8=B1=E0=
=B8=99 Nil)
local function getHRP()
=9local char =3D player.Character or player.CharacterAdded:Wait()
=9return char:FindFirstChild("HumanoidRootPart")
end

-- =F0=9F=94=8E =E0=B8=AB=E0=B8=B2 Part =E0=B8=A8=E0=B8=B1=E0=B8=95=E0=B8=
=A3=E0=B8=B9=E0=B8=97=E0=B8=B5=E0=B9=88=E0=B9=83=E0=B8=81=E0=B8=A5=E0=B9=
=89=E0=B8=95=E0=B8=B1=E0=B8=A7=E0=B8=97=E0=B8=B5=E0=B9=88=E0=B8=AA=E0=B8=
=B8=E0=B8=94 (HP > 0)
local function getClosestEnemyPart()
=9local closestPart
=9local shortestDistance =3D math.huge

=9for =5F, part in ipairs(enemiesFolder:GetChildren()) do
=9=9if part:IsA("Part") and part:IsDescendantOf(workspace) then
=9=9=9local hp =3D part:GetAttribute("HP")
=9=9=9if hp and hp > 0 then
=9=9=9=9local hrp =3D getHRP()
=9=9=9=9if hrp then
=9=9=9=9=9local distance =3D (part.Position - hrp.Position).Magnitude
=9=9=9=9=9if distance < shortestDistance then
=9=9=9=9=9=9shortestDistance =3D distance
=9=9=9=9=9=9closestPart =3D part
=9=9=9=9=9end
=9=9=9=9end
=9=9=9end
=9=9end
=9end

=9return closestPart
end

-- =F0=9F=A7=A9 =E0=B8=9F=E0=B8=B1=E0=B8=87=E0=B8=81=E0=B9=8C=E0=B8=8A=E0=
=B8=B1=E0=B8=99=E0=B8=A7=E0=B8=B2=E0=B8=A3=E0=B9=8C=E0=B8=9B=E0=B9=84=E0=
=B8=9B=E0=B8=A8=E0=B8=B1=E0=B8=95=E0=B8=A3=E0=B8=B9 (=E0=B8=A1=E0=B8=B5=E0=
=B8=A3=E0=B8=B0=E0=B8=9A=E0=B8=9A=E0=B8=81=E0=B8=B1=E0=B8=99=E0=B8=95=E0=
=B8=81=E0=B9=82=E0=B8=A5=E0=B8=81)
local function teleportToEnemyPart(enemyPart, offsetDistance)
=9local hrp =3D getHRP()
=9if not (enemyPart and hrp and enemyPart:IsDescendantOf(workspace)) then =
return end

=9local direction =3D (enemyPart.Position - hrp.Position).Unit
=9local distance =3D (enemyPart.Position - hrp.Position).Magnitude
=9local targetPos =3D enemyPart.Position - direction * offsetDistance

=9-- =F0=9F=92=A1 =E0=B8=96=E0=B9=89=E0=B8=B2=E0=B9=84=E0=B8=81=E0=B8=A5=
=E0=B9=80=E0=B8=81=E0=B8=B4=E0=B8=99 300 stud =E0=B9=83=E0=B8=AB=E0=B9=89=
=E0=B9=80=E0=B8=97=E0=B9=80=E0=B8=A5=E0=B8=9E=E0=B8=AD=E0=B8=A3=E0=B9=8C=
=E0=B8=95=E0=B8=97=E0=B8=B1=E0=B8=99=E0=B8=97=E0=B8=B5
=9if distance > 300 or hrp.Position.Y < -10 then
=9=9hrp.CFrame =3D CFrame.new(targetPos + Vector3.new(0, 5, 0))
=9=9return
=9end

=9-- =F0=9F=8C=80 Tween =E0=B8=82=E0=B8=A2=E0=B8=B1=E0=B8=9A=E0=B9=80=E0=
=B8=82=E0=B9=89=E0=B8=B2=E0=B9=83=E0=B8=81=E0=B8=A5=E0=B9=89
=9if currentTween then currentTween:Cancel() end
=9local tweenTime =3D math.clamp(distance / (farmSpeed * 100), 0.1, 1)
=9local tweenInfo =3D TweenInfo.new(tweenTime, Enum.EasingStyle.Linear)
=9currentTween =3D TweenService:Create(hrp, tweenInfo, {CFrame =3D CFrame.=
new(targetPos)})
=9currentTween:Play()
=9task.wait(tweenTime)
end

--------------------------------------------------------
-- =E2=9A=94=EF=B8=8F =E0=B8=9F=E0=B8=B1=E0=B8=87=E0=B8=81=E0=B9=8C=E0=B8=
=8A=E0=B8=B1=E0=B8=99=E0=B8=AB=E0=B8=A5=E0=B8=B1=E0=B8=81 Auto Farm
--------------------------------------------------------

local function autoFarm()
=9while AutoFarmEnabled do
=9=9local hrp =3D getHRP()
=9=9if not hrp then
=9=9=9task.wait(1)
=9=9=9continue
=9=9end

=9=9local enemyPart =3D getClosestEnemyPart()
=9=9if enemyPart then
=9=9=9teleportToEnemyPart(enemyPart, 5)

=9=9=9-- =F0=9F=94=81 =E0=B8=95=E0=B8=B5=E0=B8=88=E0=B8=99=E0=B8=A8=E0=B8=
=B1=E0=B8=95=E0=B8=A3=E0=B8=B9=E0=B8=95=E0=B8=B2=E0=B8=A2
=9=9=9while AutoFarmEnabled do
=9=9=9=9if not (enemyPart and enemyPart.Parent) then break end
=9=9=9=9local hp =3D enemyPart:GetAttribute("HP")
=9=9=9=9if not hp or hp <=3D 0 then
=9=9=9=9=9task.wait(0.2)
=9=9=9=9=9break
=9=9=9=9end
=9=9=9=9task.wait(0.05)
=9=9=9end
=9=9else
=9=9=9task.wait(0.5)
=9=9end
=9=9task.wait(0.05)
=9end
end

--------------------------------------------------------
-- =E2=99=BB=EF=B8=8F =E0=B8=9F=E0=B8=B1=E0=B8=87=E0=B8=81=E0=B9=8C=E0=B8=
=8A=E0=B8=B1=E0=B8=99=E0=B9=80=E0=B8=A3=E0=B8=B4=E0=B9=88=E0=B8=A1=E0=B8=
=AB=E0=B8=A5=E0=B8=B1=E0=B8=87=E0=B8=A3=E0=B8=B5=E0=B9=80=E0=B8=81=E0=B8=
=B4=E0=B8=94
--------------------------------------------------------

local function safeStartAutoFarm()
=9task.defer(function()
=9=9while true do
=9=9=9if AutoFarmEnabled then
=9=9=9=9local hrp =3D getHRP()
=9=9=9=9if hrp then
=9=9=9=9=9task.spawn(autoFarm)
=9=9=9=9=9break
=9=9=9=9end
=9=9=9end
=9=9=9task.wait(1)
=9=9end
=9end)
end

player.CharacterAdded:Connect(function(newChar)
=9task.wait(1)
=9character =3D newChar
=9humanoidRootPart =3D newChar:WaitForChild("HumanoidRootPart")

=9if AutoFarmEnabled then
=9=9task.wait(0.5)
=9=9safeStartAutoFarm()
=9end
end)

